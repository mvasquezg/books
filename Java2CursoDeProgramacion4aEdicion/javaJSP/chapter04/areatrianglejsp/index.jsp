<html>
    <head>
        <title>
            Area de un triangulo
        </title>
    </head>
    <body>
        <h1>Area de un triangulo</h1>
        <%!
            double base=11.5, altura=3.0, area=0.0;
        %>
        <%
          area=base*altura/2;  
            out.print("<br>"+area);
        %>
    </body>
</html>